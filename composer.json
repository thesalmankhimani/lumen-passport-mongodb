{
    "name": "dusterio/lumen-passport",
    "type": "library",
    "description": "Making Laravel Passport work with Lumen",
    "keywords": ["php","lumen","laravel passport"],
    "homepage": "https://github.com/dusterio/lumen-passport",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mysenko",
            "email": "denis@mysenko.com",
            "homepage": "https://www.mysenko.com"
        }
    ],
	"repositories": [
	    {
			"type": "vcs",
			"url": "https://github.com/kayrules/passport.git"
	    }
	],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "~5.3|~5.4",
        "illuminate/support": "~5.3|~5.4",
        "laravel/passport": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dusterio\\LumenPassport\\": "src/"
        }
    }
}
